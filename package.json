{
  "name": "ngxs",
  "version": "1.5.3",
  "description": "State Mangement for Angular",
  "main": "index.js",
  "scripts": {
    "start": "ng serve --aot",
    "test": "ng test",
    "test:integration": "npm run build && npm link ./dist && ng build --preserve-symlinks --app integration --prod",
    "test:ci": "ng test -cc --browsers ChromeHeadless --single-run -cc && npm run test:integration",
    "build": "ng-packagr -p package.json",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prettier": "prettier --write \"**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://amcdnl@github.com/amcdnl/ngxs.git"
  },
  "keywords": ["ngrx", "redux", "state", "store", "state-mangement"],
  "author": "Austin McDaniel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amcdnl/ngxs/issues"
  },
  "homepage": "https://github.com/amcdnl/ngxs#readme",
  "peerDependencies": {
    "@angular/core": "^5.0.0",
    "rxjs": "^5.5.6"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.2",
    "@angular/core": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "@types/jasmine": "^2.8.0",
    "husky": "^0.15.0-rc.8",
    "jasmine": "^2.8.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^7.0.0",
    "ng-packagr": "^2.1.0",
    "prettier": "^1.11.1",
    "redux-devtools-extension": "^2.13.2",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.6",
    "tsickle": "^0.27.2",
    "tslint": "^5.8.0",
    "typescript": "~2.6.2",
    "zone.js": "^0.8.4"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts",
      "flatModuleFile": "ngxs-api"
    }
  }
}
