{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@ngxs/*": ["../../builds/*"]
    },
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "inlineSources": true,
    "skipLibCheck": true,
    "importHelpers": true,
    "lib": ["dom", "es2015"]
  }
}
