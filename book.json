{
    "gitbook": "3.x.x",
    "title": "NGXS",
    "root": "/docs",
    "plugins": [
        "highlight",
        "github",
        "include-codeblock",
        "versions",
        "anchors",
        "youtube",
        "githubcontributors"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/ngxs/store/"
        },
        "theme-api": {
            "theme": "dark"
        },
        "versions": {
            "type": "tags"
        },
        "githubcontributors": {
          "githubOwner": "ngsx",
          "githubRepository": "store"
        }
    }
}
